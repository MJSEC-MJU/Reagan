{
  "url": "https://iptvextreme.org",
  "html": "<html lang=\"it\"><head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>IPTV Extreme</title>\n    <style>\n        body, html {\n            margin: 0;\n            padding: 0;\n            overflow: hidden;\n            width: 100%;\n            height: 100%;\n            background: linear-gradient(to bottom, #0e0e0e, #2c2c2c); /* Sfondo scuro sfumato */\n        }\n\n        .lightning {\n            position: fixed;\n            top: 0;\n            left: 0;\n            width: 100%;\n            height: 100%;\n            background: rgba(255, 255, 255, 0.8);\n            opacity: 0;\n            pointer-events: none;\n            transition: opacity 0.1s ease-out;\n        }\n\n        .rain-drop {\n            position: absolute;\n            width: 2px;\n            height: 20px;\n            background: linear-gradient(to bottom, rgba(255, 255, 255, 0.8), rgba(255, 255, 255, 0.2)); /* Gradiente per il riflesso */\n            border-radius: 50%;\n            opacity: 0.7;\n            filter: blur(0.5px); /* Sfoca leggermente la goccia */\n            animation: fall linear infinite;\n        }\n\n        @keyframes fall {\n            from {\n                transform: translateY(-100%);\n                opacity: 1;\n            }\n            to {\n                transform: translateY(100vh);\n                opacity: 0.1; /* Dissolvenza verso il basso */\n            }\n        }\n\n        .info {\n            position: absolute;\n            bottom: 10px;\n            left: 10px;\n            color: white;\n            font-family: Arial, sans-serif;\n            font-size: 14px;\n            background: rgba(0, 0, 0, 0.5);\n            padding: 10px;\n            border-radius: 5px;\n        }\n\n        .skull {\n            position: absolute;\n            top: 50%;\n            left: 50%;\n            transform: translate(-50%, -50%);\n            width: 100%;\n            height: 100%;\n            background-image: url('skull.svg');\n            background-size: contain;\n            background-repeat: no-repeat;\n            background-position: center;\n            opacity: 0;\n            transition: opacity 0.1s ease-out;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"lightning\" id=\"lightning\"></div>\n    <div class=\"skull\" id=\"skull\"></div>\n    <div class=\"info\" id=\"client-info\">\n                    <strong>Indirizzo IP:</strong> 221.141.102.40<br>\n                    <strong>Posizione:</strong> Cheongju-si, North Chungcheong, South Korea<br>\n                    <strong>Fuso orario:</strong> Asia/Seoul\n                </div>\n\n    <script>\n        // Funzione per generare la pioggia\n        const createRain = () => {\n            const rainContainer = document.createDocumentFragment();\n\n            // Numero di gocce\n            const dropCount = 200;\n\n            for (let i = 0; i < dropCount; i++) {\n                const drop = document.createElement('div');\n                drop.classList.add('rain-drop');\n\n                // Posizione orizzontale casuale\n                const startX = Math.random() * window.innerWidth;\n\n                // Dimensioni casuali delle gocce\n                const width = Math.random() * 1.5 + 1; // Larghezza tra 1px e 2.5px\n                const height = Math.random() * 15 + 10; // Altezza tra 10px e 25px\n\n                // Durata e ritardo casuali\n                const duration = Math.random() * 2 + 1; // Durata tra 1 e 3 secondi\n                const delay = Math.random() * 5; // Ritardo tra 0 e 5 secondi\n\n                // Applica gli stili\n                drop.style.left = `${startX}px`;\n                drop.style.width = `${width}px`;\n                drop.style.height = `${height}px`;\n                drop.style.animationDuration = `${duration}s`;\n                drop.style.animationDelay = `${delay}s`;\n\n                rainContainer.appendChild(drop);\n            }\n\n            // Aggiungi tutte le gocce al body\n            document.body.appendChild(rainContainer);\n        };\n\n        // Funzione per simulare fulmini con il teschio\n        const createLightning = () => {\n            const lightningElement = document.getElementById('lightning');\n            const skullElement = document.getElementById('skull');\n\n            const flashLightning = () => {\n                const flashes = Math.floor(Math.random() * 3) + 2; // Numero di lampi (2-4)\n                let delay = 0;\n\n                for (let i = 0; i < flashes; i++) {\n                    setTimeout(() => {\n                        lightningElement.style.opacity = '1';\n                        skullElement.style.opacity = '1'; // Mostra il teschio\n                        setTimeout(() => {\n                            lightningElement.style.opacity = '0';\n                            if (i === flashes - 1) skullElement.style.opacity = '0'; // Nascondi il teschio dopo l'ultimo lampo\n                        }, 100); // Durata di ciascun lampo\n                    }, delay);\n\n                    delay += Math.random() * 200 + 100; // Ritardo casuale tra i lampi\n                }\n            };\n\n            const randomInterval = Math.random() * 5000 + 2000; // Intervallo casuale tra 2 e 7 secondi\n            setTimeout(() => {\n                flashLightning();\n                createLightning(); // Richiama la funzione per fulmini ricorrenti\n            }, randomInterval);\n        };\n\n        // Funzione per ottenere informazioni sull'IP e la posizione\n        const fetchClientInfo = async () => {\n            try {\n                const response = await fetch('https://ipapi.co/json/');\n                if (!response.ok) throw new Error('Errore nel recupero delle informazioni');\n                const data = await response.json();\n\n                const infoElement = document.getElementById('client-info');\n                infoElement.innerHTML = `\n                    <strong>Indirizzo IP:</strong> ${data.ip}<br>\n                    <strong>Posizione:</strong> ${data.city}, ${data.region}, ${data.country_name}<br>\n                    <strong>Fuso orario:</strong> ${data.timezone}\n                `;\n            } catch (error) {\n                document.getElementById('client-info').innerText = 'Impossibile recuperare le informazioni del client.';\n                console.error(error);\n            }\n        };\n\n        // Avvia l'effetto pioggia\n        createRain();\n\n        // Avvia l'effetto fulmini\n        createLightning();\n\n        // Recupera e mostra le informazioni del client\n        fetchClientInfo();\n    </script><div class=\"rain-drop\" style=\"left: 603.53px; width: 1.43329px; height: 13.937px; animation-duration: 2.92184s; animation-delay: 2.4228s;\"></div><div class=\"rain-drop\" style=\"left: 367.18px; width: 2.38966px; height: 14.4862px; animation-duration: 1.06789s; animation-delay: 3.32372s;\"></div><div class=\"rain-drop\" style=\"left: 161.344px; width: 2.20645px; height: 22.8169px; animation-duration: 1.49167s; animation-delay: 1.66004s;\"></div><div class=\"rain-drop\" style=\"left: 201.424px; width: 1.09361px; height: 19.9592px; animation-duration: 2.19967s; animation-delay: 4.11401s;\"></div><div class=\"rain-drop\" style=\"left: 50.6147px; width: 2.2623px; height: 16.9593px; animation-duration: 2.77907s; animation-delay: 2.38222s;\"></div><div class=\"rain-drop\" style=\"left: 562.041px; width: 1.15737px; height: 19.4519px; animation-duration: 2.09063s; animation-delay: 2.30987s;\"></div><div class=\"rain-drop\" style=\"left: 814.754px; width: 1.16012px; height: 20.3226px; animation-duration: 2.39181s; animation-delay: 1.68519s;\"></div><div class=\"rain-drop\" style=\"left: 925.34px; width: 1.66639px; height: 19.9598px; animation-duration: 2.2924s; animation-delay: 3.72996s;\"></div><div class=\"rain-drop\" styl