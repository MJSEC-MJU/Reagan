erHTML = cust_name_dfXnjj34;\n              }\n            }\n          }\n        }\n      }\n    </script>\n    \n    \n\n\n\t\t<style type=\"text/css\">\n\n    .word {\n        opacity: 0.2;\n        color: black;\n        transition: opacity 0.3s ease, color 0.3s ease;\n    }\n\n    .word.active {\n        opacity: 1;\n        color: red;\n    }\n\n    .word.fullstop {\n        display: inline; /* Ensure it appears directly after the last word */\n    }\t\n     .word {\n        white-space: nowrap;\n    }\n    \n    .highlight-text span, .highlight-text p {\n    font-weight: inherit;\n}\n</style><script src=\"https://code.jquery.com/jquery-3.7.1.min.js\" integrity=\"sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=\" crossorigin=\"anonymous\"></script>\n<script>\n    $(document).ready(function() {\n        // Iterate over each element with the .highlight-text class\n        $(\".highlight-text\").each(function() {\n            var el = $(this),\n                content = el.html().trim(), // Trim any extra spaces\n                lines = content.match(/[^.?!]+[.?!]?/g); // Match sentences including punctuation\n\n            var html = \"\";\n\n            // Split content into paragraphs and each paragraph into individual words\n            lines.forEach((line) => {\n                html += \"<p class='clear-display line'>\";\n                let words = line.trim().split(\" \");\n                words.forEach((word, wordIndex) => {\n                    html += \"<span class='word' style='transition-delay:\" + (wordIndex * 0.1) + \"s'>\" + word + \"</span>\";\n                    // Add a non-breaking space after each word except the last word in the line\n                    if (wordIndex < words.length - 1) {\n                        html += \"<span class='space'>&nbsp;</span>\";\n                    }\n                });\n                html += \"</p>\";\n            });\n\n            // Replace the content of the current .highlight-text element\n            el.html(html);\n        });\n\n        var activationOffset = $(window).height() * 0.8; // Highlight at 20% from the bottom of the screen\n\n        function calledEveryScroll() {\n            var scrollPosition = $(window).scrollTop();\n\n            // Activate or deactivate each word based on scroll position\n            $(\".word\").each(function() {\n                var wordTop = $(this).offset().top;\n\n                // Add 'active' class when word is in view; remove it when scrolled up past threshold\n                if (scrollPosition + activationOffset >= wordTop) {\n                    $(this).addClass('active');\n                } else {\n                    $(this).removeClass('active');\n                }\n            });\n        }\n\n        function resizing() {\n            var offset = 100;\n            var bottom = $(window).height() - offset;\n        }\n\n        $(window).scroll(function() {\n            calledEveryScroll();\n        });\n\n        $(window).resize(function() {\n            resizing();\n        });\n\n        // Call function once on load to highlight initial visible words\n        calledEveryScroll();\n    });\n</script></body></html>",
