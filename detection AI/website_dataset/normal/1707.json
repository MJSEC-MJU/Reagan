件状态\n        let currentIndex = 0;\n        let isActive = false;\n        let timer = null;\n\n        function clampIndex(index) {\n          return (index + config.totalSlides) % config.totalSlides;\n        }\n\n        function updateCarousel() {\n          $carousel.css(\"transform\", `translateX(-${currentIndex * 100}%)`);\n        }\n\n        function autoSlide() {\n          if (!isActive) return;\n          currentIndex = clampIndex(currentIndex + 1);\n          updateCarousel();\n        }\n\n        function startTimer() {\n          clearInterval(timer);\n          timer = setInterval(autoSlide, config.autoPlayInterval);\n        }\n\n        function handleIntersection(entries) {\n          entries.forEach((entry) => {\n            isActive =\n              entry.isIntersecting &&\n              entry.intersectionRatio >= config.viewportThreshold;\n            isActive ? startTimer() : clearInterval(timer);\n          });\n        }\n\n        function handleMouseEnter() {\n          isActive = false;\n        }\n        function handleMouseExit() {\n          isActive = true;\n        }\n        function handlePrevClick() {\n          currentIndex = clampIndex(currentIndex - 1);\n          updateCarousel();\n          startTimer();\n        }\n        function handleNextClick() {\n          currentIndex = clampIndex(currentIndex + 1);\n          updateCarousel();\n          startTimer();\n        }\n\n        function initialize() {\n          const observer = new IntersectionObserver(handleIntersection, {\n            threshold: config.viewportThreshold,\n          });\n          $carousel.hover(handleMouseEnter, handleMouseExit);\n          $prevBtn.click(handlePrevClick);\n          $nextBtn.click(handleNextClick);\n          observer.observe(document.getElementById(\"section3_cut\"));\n        }\n        return { initialize };\n      }\n\n      if (!getOs.isMobile()) {\n        const carousel = new CarouselController();\n        carousel.initialize();\n      }\n\n      const banner_video = document.getElementById(\"banner_video\");\n      const before_after_video = document.getElementById(\"before_after_video\");\n      const observer = new IntersectionObserver(\n        (entries) => {\n          entries.forEach((entry) => {\n            entry.isIntersecting ? entry.target.play() : entry.target.pause();\n          });\n        },\n        {\n          threshold: 0.6,\n        }\n      );\n\n      if (\"IntersectionObserver\" in window) {\n        observer.observe(banner_video);\n        observer.observe(before_after_video);\n      } else {\n        banner_video.play();\n        before_after_video.play();\n      }\n    </script>\n  \n\n<div class=\"sprtadv adv_rigth\" style=\"bottom: 46px; top: auto;\">\n                    <span class=\"stadv_pcbot_close\"></span>\n                    <a class=\"stadv_pct\" href=\"https://gemoo.com/wipit/?ref=adpc&amp;source=adv_pc_gemoo&amp;orgin=/index\">\n                       <img id=\"pcAdv\" src=\"https://www.gemoo-resource.com/img/ad2wipit_adpc_footright1.png\" width=\"300\">\n                    </a>\n                </div></body></html>",
